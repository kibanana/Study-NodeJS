{
  "presets": [
    [
    "@babel/preset-env",
    {
      "modules": "auto",
      "targets": {
        "browsers": ["defaults"]
      },
      "useBuiltIns": "entry"
    }
    ],
    "@babel/preset-flow"
    ],
    "plugins": 
    [
      "@babel/plugin-proposal-class-properties",
      "@babel/transform-runtime",
      "add-module-exports",
      "babel-plugin-dynamic-import-node"
    ]
}